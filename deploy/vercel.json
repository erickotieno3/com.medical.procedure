{
  "version": 2,
  "name": "medical-ai-assistant",
  "builds": [
    {
      "src": "api/medical-ai.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/medical-ai",
      "dest": "/api/medical-ai.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
