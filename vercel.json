{
  "version": 2,
  "name": "medical-procedure-ai",
  "builds": [
    {
      "src": "api/**.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "EXPO_USE_FAST_REFRESH": "true"
  },
  "buildCommand": "expo export:web"
}
